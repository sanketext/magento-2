<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Excelss\Customerproducts\Block\Customer\Account\Link" name="quick-order-link" after="-">
                <arguments>
                    <argument name="path" xsi:type="string">customerproducts/quickorder/index</argument>
                    <argument name="label" translate="true" xsi:type="string">Quick Order</argument>
                </arguments>
            </block>
            <block class="Excelss\Customerproducts\Block\Customer\Account\Link" name="manage-order" after="-">
                <arguments>
                    <argument name="path" xsi:type="string">customerproducts/quickorder/manage</argument>
                    <argument name="label" translate="true" xsi:type="string">Manage Reorder Product</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>

